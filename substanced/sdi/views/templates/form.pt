<div metal:use-macro="request.sdiapi.main_template">

  <metal:head-first fill-slot="head-first">
    <!-- CSS -->
    <link rel="stylesheet" 
          href="${request.static_url('deform:static/css/form.css')}"
          type="text/css"/>
    <tal:block repeat="reqt css_links|[]">
      <link rel="stylesheet" 
            href="${request.static_url(reqt)}" 
            type="text/css" />
     </tal:block>

    <!-- JavaScript -->
    <tal:block repeat="reqt js_links|[]">
      <script type="text/javascript"
              src="${request.static_url(reqt)}"
       ></script>
    </tal:block>
  </metal:head-first>

  <div metal:fill-slot="main">

    <div class="container">
      <div class="well">
        <h1>${view.title|None}</h1>
        <div class="row">
          <div class="col-md-6" id="form" tal:content="structure form"/>
          </div>
        </div>
      </div>

  </div>

</div>

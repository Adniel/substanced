<div metal:use-macro="view.macros['layout']">
    <div metal:fill-slot="main">
        <h1>${view.title}</h1>

        <div class="container-fluid">
            <ul>
                <li tal:repeat="topic topics">
                    <a href="${topic.url}">
                        ${topic.title}
                    </a>
                </li>
            </ul>
        </div>

    </div>
</div>
